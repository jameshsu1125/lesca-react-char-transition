{
  "name": "lesca-react-char-transition",
  "version": "1.0.6",
  "description": "easy way to transition char code",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "watch": "babel --watch src/lib --out-dir lib --extensions '.ts,.tsx'",
    "build": "NODE_ENV=production webpack",
    "less": "less-watch-compiler",
    "css": "copy-and-watch --watch src/lib/*.css lib/",
    "deploy": "gh-pages -d dist",
    "tsc": "npx tsc --watch",
    "dev": "concurrently \"npm run tsc\" \"npm run less\" \"npm run css\" \"npm run watch\" \"npm run start\"",
    "rm": "rm -rf node_modules/ package-lock.json",
    "op": "babel src/lib -d lib --copy-files"
  },
  "keywords": [
    "lesca"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-and-watch": "^0.1.8",
    "css-loader": "^7.1.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-react-parser": "^5.1.10",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "less-vars-to-js": "^1.3.0",
    "less-watch-compiler": "^1.16.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "author": "jamehsu1125 <jameshsu1125@gmail.com>",
  "homepage": "https://github.com/jameshsu1125/npm-base-package",
  "repository": {
    "type": "git",
    "url": "git@github.com:jameshsu1125/npm-base-package.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "lesca-object-tweener": "^2.4.7"
  }
}
